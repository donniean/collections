{
  "name": "collections",
  "version": "1.0.0",
  "description": "",
  "homepage": "https://github.com/donniean/collections#readme",
  "bugs": {
    "url": "https://github.com/donniean/collections/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donniean/collections.git"
  },
  "license": "MIT",
  "author": "Donnie An",
  "type": "module",
  "scripts": {
    "commit": "cz",
    "docker:ip-quality:build": "docker build --tag donniean/ip-quality ./docker/IPQuality",
    "docker:ip-quality:commit": "docker commit IPQuality donniean/ip-quality",
    "docker:ip-quality:pull": "docker pull donniean/ip-quality",
    "docker:ip-quality:push": "docker push donniean/ip-quality",
    "docker:ip-quality:run": "docker run --name IPQuality -it --rm donniean/ip-quality",
    "preinstall": "npx only-allow npm",
    "knip": "knip",
    "lint-all": "concurrently --group --timings --prefix-colors=auto \"npm:lint:*(!:fix)\"",
    "lint-all:fix": "concurrently --max-processes=1 --group --timings --prefix-colors=auto \"npm:lint:*:fix\"",
    "lint:autocorrect": "autocorrect --lint",
    "lint:autocorrect:fix": "autocorrect --fix",
    "lint:cspell": "cspell lint --no-progress --relative --no-must-find-files --dot --gitignore \"**\"",
    "lint:eslint": "eslint \"**/*.{js,mjs,cjs,ts}\"",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:htmlhint": "htmlhint --ignore=\"**/coverage/**\" \"**/*.html\"",
    "lint:markdownlint": "markdownlint --dot \"**/*.md\"",
    "lint:markdownlint:fix": "npm run lint:markdownlint -- --fix",
    "lint:prettier": "prettier --check --ignore-unknown \"**\"",
    "lint:prettier:fix": "prettier --write --ignore-unknown \"**\"",
    "lint:sort-package-json": "npm run lint:sort-package-json:fix -- --check",
    "lint:sort-package-json:fix": "sort-package-json",
    "lint:stylelint": "stylelint \"**/*.css\"",
    "lint:stylelint:fix": "npm run lint:stylelint -- --fix",
    "lint:tsc": "tsc --noEmit",
    "ncu": "ncu --deep",
    "ncu:upgrade": "npm run ncu -- --upgrade",
    "pre-commit": "lint-staged --concurrent false",
    "prepare": "husky",
    "serve": "serve snippets/styles/"
  },
  "dependencies": {
    "@notionhq/client": "^3.0.1",
    "dotenv-flow": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@donniean/configs": "^6.24.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.26.0",
    "@tsconfig/node-lts": "^22.0.1",
    "@types/node": "^22.15.17",
    "@vitest/eslint-plugin": "^1.1.44",
    "autocorrect-node": "^2.14.0",
    "commitizen": "^4.3.1",
    "concurrently": "^9.1.2",
    "cspell": "^9.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import-x": "^4.11.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.1.0",
    "htmlhint": "^1.1.4",
    "husky": "^9.1.7",
    "knip": "^5.55.1",
    "lint-staged": "^16.0.0",
    "markdownlint-cli": "^0.44.0",
    "npm-check-updates": "^18.0.1",
    "prettier": "^3.5.3",
    "serve": "^14.2.4",
    "sort-package-json": "^3.2.1",
    "stylelint": "^16.19.1",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-standard": "^38.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
