{
  "name": "collections",
  "version": "1.0.0",
  "description": "",
  "homepage": "https://github.com/donniean/collections#readme",
  "bugs": {
    "url": "https://github.com/donniean/collections/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donniean/collections.git"
  },
  "license": "MIT",
  "author": "Donnie An",
  "type": "module",
  "scripts": {
    "commit": "cz",
    "preinstall": "npx only-allow npm",
    "lint:cspell": "cspell lint --no-progress --relative --no-must-find-files --dot --gitignore \"**\"",
    "lint:eslint": "eslint \"**/*.{js,mjs,cjs}\"",
    "lint:eslint:fix": "eslint --fix \"**/*.{js,mjs,cjs}\"",
    "lint:markdownlint": "markdownlint --dot \"**/*.md\"",
    "lint:markdownlint:fix": "markdownlint --dot --fix \"**/*.md\"",
    "lint:prettier": "prettier --check --ignore-unknown \"**\"",
    "lint:prettier:fix": "prettier --write --ignore-unknown \"**\"",
    "lint:sort-package-json": "sort-package-json --check",
    "lint:sort-package-json:fix": "sort-package-json",
    "pre-commit": "lint-staged --concurrent false",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@types/eslint": "^8.44.4",
    "commitizen": "^4.3.0",
    "cspell": "^7.3.8",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "markdownlint-cli": "^0.37.0",
    "prettier": "^3.0.3",
    "sort-package-json": "^2.6.0"
  }
}
