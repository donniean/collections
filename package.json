  "type": "module",
  "scripts": {
    "commit": "cz",
    "preinstall": "npx only-allow npm",
    "lint:cspell": "cspell lint --no-progress --relative --no-must-find-files --dot --gitignore \"**\"",
    "lint:eslint": "eslint \"**/*.{js,mjs,cjs,ts}\"",
    "lint:eslint:fix": "eslint --fix \"**/*.{js,mjs,cjs,ts}\"",
    "lint:markdownlint": "markdownlint --dot \"**/*.md\"",
    "lint:markdownlint:fix": "markdownlint --dot --fix \"**/*.md\"",
    "lint:prettier": "prettier --check --ignore-unknown \"**\"",
    "lint:prettier:fix": "prettier --write --ignore-unknown \"**\"",
    "lint:sort-package-json": "sort-package-json --check",
    "lint:sort-package-json:fix": "sort-package-json",
    "lint:tsc": "tsc --noEmit",
    "pre-commit": "lint-staged --concurrent false",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@tsconfig/node-lts": "^20.1.1",
    "@types/eslint": "^8.56.2",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "commitizen": "^4.3.0",
    "cspell": "^8.3.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-unicorn": "^50.0.1",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.1",
    "markdownlint-cli": "^0.39.0",
    "prettier": "^3.2.5",
    "sort-package-json": "^2.7.0",
    "typescript": "^5.3.3"
